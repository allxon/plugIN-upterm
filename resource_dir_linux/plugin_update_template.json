{
  "jsonrpc": "2.0",
  "method": "v2/notifyPluginUpdate",
  "params": {
    "appGUID": "${PLUGIN_APP_GUID}",
    "epoch": "",
    "sdk": "${OCTO_SDK_VERSION}",
    "appName": "${PLUGIN_NAME}",
    "displayName": "${PLUGIN_NAME}",
    "type": "ib",
    "version": "${PLUGIN_VERSION}",
    "modules": [
      {
        "moduleName": "${PLUGIN_NAME}",
        "displayName": "Upterm",
        "properties": [
          {
            "name": "officialSite",
            "displayName": "Powered by",
            "description": "",
            "displayType": "link",
            "value":
            {
              "url": "https://upterm.dev",
              "alias": "upterm.dev"
            }
          },
          {
            "name": "license",
            "displayName": "LICENSE",
            "description": "",
            "displayType": "link",
            "value":
            {
              "url": "https://github.com/allxon/plugIN-upterm/blob/master/LICENSE",
              "alias": "The 3-Clause BSD License"
            }
  
          }
        ],
        "states": [
          {
            "name": "version",
            "displayName": "Version",
            "description": "The software version of upterm.",
            "displayType": "string"
          },
          {
            "name": "status",
            "displayName": "Session",
            "description": "Running status of upterm.",
            "displayType": "string"
          },
          {
            "name": "ssh",
            "displayName": "SSH",
            "description": "Copy and paste the SSH key in local terminal",
            "displayType": "string"
          }
        ],
        "commands": [
          {
            "name": "start",
            "type": "asynchronous",
            "displayCategory": "Session",
            "displayName": "Open session",
            "description": "Open a upterm session to enable instant terminal sharing.",
            "params": [
              {
                "name": "password",
                "displayName": "Password",
                "description": "Set a password for the user account upterm",
                "displayType": "string",
                "required": true,
                "defaultValue": "upterm"
              },
              {
                "name": "server_url",
                "displayName": "Server",
                "description": "upterm server address (required)",
                "displayType": "string",
                "required": true,
                "defaultValue": "ssh://uptermd.upterm.dev:22"
              }
            ]
          },
          {
            "name": "stop",
            "type": "asynchronous",
            "displayCategory": "Session",
            "displayName": "Close session",
            "description": "Close the upterm session to terminate terminal sharing.",
            "params": []
          },
          {
            "name": "install",
            "type": "asynchronous",
            "displayCategory": "Installation",
            "displayName": "Install upterm",
            "description": "Install upterm to be able to do instant terminal sharing.",
            "params": []
          },
          {
            "name": "uninstall",
            "type": "asynchronous",
            "displayCategory": "Installation",
            "displayName": "Uninstall upterm",
            "description": "Uninstall upterm.",
            "params": []
          }
        ],
        "metrics": [],
        "events": [],
        "alarms": [],
        "configs": []
      }
    ]
  }
}