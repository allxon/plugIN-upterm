{
  "jsonrpc": "2.0",
  "method": "v2/notifyPluginUpdate",
  "params": {
    "appGUID": "${PLUGIN_APP_GUID}",
    "epoch": "",
    "sdk": "${OCTO_SDK_VERSION}",
    "appName": "${PLUGIN_NAME}",
    "displayName": "Upterm Add-in",
    "type": "ib",
    "version": "${PLUGIN_VERSION}",
    "modules": [
      {
        "moduleName": "${PLUGIN_NAME}",
        "displayName": "Upterm Add-in",
        "properties": [
          {
            "name": "officialSite",
            "displayName": "i-m-upterm-p-officialSite",
            "displayType": "link",
            "value":
            {
              "url": "https://upterm.dev",
              "alias": "upterm.dev"
            }
          },
          {
            "name": "license",
            "displayName": "i-m-upterm-p-license",
            "displayType": "link",
            "value":
            {
              "url": "https://github.com/allxon/plugIN-upterm/blob/master/LICENSE",
              "alias": "The 3-Clause BSD License"
            }
  
          }
        ],
        "states": [
          {
            "name": "version",
            "displayName": "i-m-upterm-s-version",
            "description": "i-m-upterm-s-version-d",
            "displayType": "string"
          },
          {
            "name": "status",
            "displayName": "i-m-upterm-s-status",
            "description": "i-m-upterm-s-status-d",
            "displayType": "string"
          },
          {
            "name": "ssh",
            "displayName": "i-m-upterm-s-ssh",
            "description": "i-m-upterm-s-ssh-d",
            "displayType": "string"
          }
        ],
        "commands": [
          {
            "name": "start",
            "type": "asynchronous",
            "displayCategory": "i-m-upterm-c-start-dc",
            "displayName": "i-m-upterm-c-start",
            "description": "i-m-upterm-c-start-d",
            "params": [
              {
                "name": "password",
                "displayName": "i-m-upterm-c-p-password",
                "description": "i-m-upterm-c-p-password-d",
                "displayType": "string",
                "required": true,
                "defaultValue": "upterm"
              },
              {
                "name": "server_url",
                "displayName": "i-m-upterm-c-p-server_url",
                "description": "i-m-upterm-c-p-server_url-d",
                "displayType": "string",
                "required": true,
                "defaultValue": "ssh://uptermd.upterm.dev:22"
              }
            ]
          },
          {
            "name": "stop",
            "type": "asynchronous",
            "displayCategory": "i-m-upterm-c-stop-dc",
            "displayName": "i-m-upterm-c-stop",
            "description": "i-m-upterm-c-stop-d",
            "params": []
          },
          {
            "name": "install",
            "type": "asynchronous",
            "displayCategory": "i-m-upterm-c-install-dc",
            "displayName": "i-m-upterm-c-install",
            "description": "i-m-upterm-c-install-d",
            "params": []
          },
          {
            "name": "uninstall",
            "type": "asynchronous",
            "displayCategory": "i-m-upterm-c-uninstall-dc",
            "displayName": "i-m-upterm-c-uninstall",
            "description": "i-m-upterm-c-uninstall-d",
            "params": []
          }
        ],
        "metrics": [],
        "events": [],
        "alarms": [],
        "configs": []
      }
    ]
  }
}